<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	
	    <servlet>
         <servlet-name>regRegisterServlet</servlet-name>
         <servlet-class>com.online.quiz.servlets.RegisterServlet</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regRegisterServlet</servlet-name>
	         <url-pattern>/RegisterServlet</url-pattern>
	    </servlet-mapping>
	    
	    
	    
	    
	    <servlet>
         <servlet-name>regLogin</servlet-name>
         <servlet-class>com.online.quiz.servlets.Login</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regLogin</servlet-name>
	         <url-pattern>/Login</url-pattern>
	    </servlet-mapping>
	    
	    <servlet>
         <servlet-name>regLogout</servlet-name>
         <servlet-class>com.online.quiz.servlets.Logout</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regLogout</servlet-name>
	         <url-pattern>/Logout</url-pattern>
	    </servlet-mapping>
	    
	    
	    
	    
	   <servlet>
         <servlet-name>regQuizServlet</servlet-name>
         <servlet-class>com.online.quiz.servlets.QuizServlet</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regQuizServlet</servlet-name>
	         <url-pattern>/QuizServlet</url-pattern>
	    </servlet-mapping>
	    
	    
	    
	    <servlet>
         <servlet-name>regQuesServlet</servlet-name>
         <servlet-class>com.online.quiz.servlets.QuesServlet</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regQuesServlet</servlet-name>
	         <url-pattern>/QuesServlet</url-pattern>
	    </servlet-mapping>
	    
	    
	    
	    
	    <servlet>
         <servlet-name>regCheckAnswerServlet</servlet-name>
         <servlet-class>com.online.quiz.servlets.CheckAnswerServlet</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regCheckAnswerServlet</servlet-name>
	         <url-pattern>/CheckAnswerServlet</url-pattern>
	    </servlet-mapping>
	    
	    
	    
	    
	     <servlet>
         <servlet-name>regSubmitQuiz</servlet-name>
         <servlet-class>com.online.quiz.servlets.SubmitQuiz</servlet-class>
        </servlet>
	    <servlet-mapping>
	         <servlet-name>regSubmitQuiz</servlet-name>
	         <url-pattern>/SubmitQuiz</url-pattern>
	    </servlet-mapping>
	    
	    
	
</web-app>